<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ResultActivity">

    <TextView
            android:text="TextView"
            android:layout_width="381dp"
            android:layout_height="80dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/intentTextView1"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp" android:textSize="30sp"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView2" app:layout_constraintVertical_bias="0.833"/>
    <TextView
            android:text="TextView"
            android:layout_width="361dp"
            android:layout_height="88dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/intentTextView2" app:layout_constraintHorizontal_bias="0.595"
            android:layout_marginEnd="8dp" android:layout_marginRight="16dp" android:textSize="30sp"
            android:lineSpacingExtra="8sp" android:layout_marginBottom="264dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="今日も１日がんばろう！"
            android:layout_width="361dp"
            android:layout_height="70dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView2"
            android:textSize="30sp" android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toTopOf="@+id/intentTextView2"/>

</androidx.constraintlayout.widget.ConstraintLayout>